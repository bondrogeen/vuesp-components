{
  "version": "1.1.2",
  "name": "vuesp-components",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.es.js",
      "default": "./dist/index.es.js"
    },
    "./plugins": {
      "types": "./dist/plugins/index.d.ts",
      "import": "./dist/plugins/index.es.js",
      "default": "./dist/plugins/index.es.js"
    },
    "./dashboard": {
      "types": "./dist/dashboard/index.d.ts",
      "import": "./dist/dashboard/index.es.js",
      "default": "./dist/dashboard/index.es.js"
    },
    "./composables": {
      "types": "./dist/composables/index.d.ts",
      "import": "./dist/composables/index.es.js",
      "default": "./dist/composables/index.es.js"
    },
    "./helpers": {
      "types": "./dist/helpers/index.d.ts",
      "import": "./dist/helpers/index.es.js",
      "default": "./dist/helpers/index.es.js"
    },
    "./types": {
      "types": "./dist/types/types.d.ts"
    },
    "./dist/style.css": {
      "import": "./dist/style.css",
      "require": "./dist/style.css"
    }
  },
  "description": "vuesp-components",
  "repository": {
    "type": "git",
    "url": "https://github.com/bondrogeen/vuesp-components"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "vite preview"
  },
  "keywords": [
    "vue",
    "component",
    "typescript"
  ],
  "author": "bondrogeen",
  "license": "MIT",
  "dependencies": {
    "@tailwindcss/vite": "4.1.6",
    "@vueuse/core": "^13.9.0",
    "tailwindcss": "4.1.6",
    "vue": "3.5.0"
  },
  "devDependencies": {
    "@types/node": "^18.11.18",
    "@vitejs/plugin-vue": "^4.5.0",
    "tsconfig-paths": "^4.2.0",
    "tslib": "^2.3.0",
    "typescript": "~5.9.2",
    "typescript-eslint": "^8.40.0",
    "vite": "^5.0.0",
    "vite-plugin-dts": "~4.5.0",
    "vite-svg-loader": "^5.1.0",
    "vue-tsc": "^2.2.8"
  },
  "peerDependencies": {
    "vue": "3.5.0"
  }
}
